        <sys:Double x:Key="GlyphSize">14</sys:Double>
        <Style TargetType="TextBlock">
            <Setter Property="Foreground" Value="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}"/>
        </Style>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#7FBEE6FD"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#7F3C7FB1"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#7FC4E5F6"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#7F2C628B"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#7FF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#7FADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#7F838383"/>
        <ControlTemplate x:Key="ToggleButtonTemplate1" TargetType="{x:Type ToggleButton}">
            <Border x:Name="border" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" CornerRadius="2" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="true">
                <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="Button.IsDefaulted" Value="true">
                    <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                </Trigger>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter Property="Background" TargetName="border" Value="{StaticResource Button.MouseOver.Background}"/>
                    <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.MouseOver.Border}"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="true">
                    <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Pressed.Background}"/>
                    <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Pressed.Border}"/>
                </Trigger>
                <Trigger Property="IsEnabled" Value="false">
                    <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Disabled.Background}"/>
                    <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Disabled.Border}"/>
                    <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                </Trigger>
                <Trigger Property="IsChecked" Value="true">
                    <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Pressed.Background}"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
    </UserControl.Resources>

    <Border CornerRadius="4"  
            BorderBrush="{DynamicResource {x:Static vs:VsBrushes.HighlightKey}}"
            BorderThickness="1"
            Padding="16">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <Grid>
                <ToggleButton Template="{DynamicResource ToggleButtonTemplate1}" x:Name="PinButton" HorizontalAlignment="Right" IsChecked="{Binding Path=(local:ProcessPanel.IsPinned), Mode=TwoWay}" Background="Transparent" Margin="0,0,0,5">
                    <Grid Width="16" Height="16">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Path x:Name="PinGlyph" Stretch="Fill" Fill="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}" Stroke="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}" StrokeThickness="1" Data="{StaticResource PinSvg}" Grid.RowSpan="2"/>
                    </Grid>
                    <ToggleButton.Style>
                        <Style TargetType="ToggleButton">
                            <Style.Triggers>
                                <DataTrigger>
                                    <DataTrigger.Binding>
                                        <Binding Source="{x:Static local:ProcessPanel.IsPinned}" Mode="OneWay"/>
                                    </DataTrigger.Binding>
                                    <Setter Property="Content">
                                        <Setter.Value>
                                            <Grid Width="14" Height="14">
                                                <Path x:Name="PinGlyph" Stretch="Fill" Stroke="Black" StrokeThickness="1" Data="{StaticResource UnpinSvg}"/>
                                            </Grid>
                                        </Setter.Value>
                                    </Setter>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </ToggleButton.Style>
                </ToggleButton>
            </Grid>

            <ListView ItemsSource="{Binding ProcessItems, Mode=OneWay}" Grid.Row="1" Background="{DynamicResource {x:Static vs:VsBrushes.ToolWindowBackgroundKey}}">
                <ListView.ItemContainerStyle>
                    <Style TargetType="ListViewItem">
                        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                    </Style>
                </ListView.ItemContainerStyle>
                <ListView.ItemTemplate>
                    <DataTemplate DataType="local:ProcessCPUUsageModel">
                        <Grid x:Name="RootGrid" Background="Transparent" MinHeight="30">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{Binding ColoredWidth}"/>
                                    <ColumnDefinition Width="{Binding UncoloredWidth}"/>
                                </Grid.ColumnDefinitions>

                                <Rectangle RadiusX="4" RadiusY="4" Fill="{StaticResource CPUColor}" Grid.Column="0"/>
                            </Grid>
                            <TextBlock Text="{Binding Name}" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}"/>

                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding PercentText, Mode=OneWay}" VerticalAlignment="Center" Foreground="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}"/>
                                <Button x:Name="KillProcessButton" Click="Button_Click" Visibility="Collapsed" Margin="4,0,0,0">
                                    <Grid Width="16" Height="16">
                                        <Path Stretch="Fill" Fill="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}" Stroke="{DynamicResource {x:Static vs:VsBrushes.StatusBarTextKey}}" StrokeThickness="1" Data="{StaticResource CancelSvg}"/>
                                    </Grid>
                                </Button>
                            </StackPanel>
                        </Grid>
                        <DataTemplate.Triggers>
                            <Trigger SourceName="RootGrid" Property="IsMouseOver" Value="True">
                                <Setter TargetName="KillProcessButton" Property="Visibility" Value="Visible"/>
                            </Trigger>
                        </DataTemplate.Triggers>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </Grid>
    </Border>

</UserControl>
